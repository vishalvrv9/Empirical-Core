{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "ordered-imports": false,
      "object-literal-sort-keys": false,
      "interface-name": false,
      "quotemark": false,
      "semicolon": false,
      "member-access": false,
      "max-line-length": false,
      "trailing-comma": false,
      "one-variable-per-declaration": false,
      "missing-whitespace": false,
      "arrow-parens": false,
      "ban-types": false,
      "no-console": false,
      "no-empty": false
    },
    "rulesDirectory": []
}
